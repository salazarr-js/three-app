{
  "name": "@slzr/three-app",
  "description": "Set of utility helpers to use `ThreeJs` in a functional and declarative way",
  "version": "0.0.1",
  "license": "MIT",
  "homepage": "https://github.com/salazarr-js/three-app",
  "repository": "(https://github.com/salazarr-js/three-app)",
  "author": "Jose Salazar",
  "type": "module",
  "files": [
    "dist",
    "package.json"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "keywords": "",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/three": "^0.168.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "three": "^0.168.0"
  }
}
