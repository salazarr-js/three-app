{
  "name": "@slzr/three-app",
  "type": "module",
  "version": "0.2.3",
  "description": "ThreeJs but functional and declarative.",
  "author": "Jos√© Salazar <jsalazar3300@gmail.com> (https://salazarjs.dev)",
  "license": "MIT",
  "homepage": "https://three.salazarjs.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salazarr-js/three-app.git",
    "directory": "packages/core"
  },
  "keywords": [
    "three",
    "three.js",
    "javascript",
    "typescript",
    "3d",
    "webgl",
    "webgl2",
    "webgpu",
    "canvas",
    "svg"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js",
      "require": "./dist/components.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "tsc": "tsc",
    "publish": "pnpm publish --access public",
    "publish:patch": "npm version patch && pnpm publish --access public"
  },
  "peerDependencies": {
    "three": "catalog:"
  },
  "dependencies": {
    "three-stdlib": "^2.35.6"
  },
  "devDependencies": {
    "@types/three": "catalog:",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
